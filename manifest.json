{
  "manifest_version": 2,
  "name": "Yet another new Tab Page",
  "version": "0.2.2",
  "author": "Ramkumar K R",
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "description":"Yet another new tab page",
  "offline_enabled": true,
  "permissions": [
    "tabs",
    "storage"
    ],
  "options_page": "options.html"
}
